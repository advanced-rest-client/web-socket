{
  "verbose": false,
  "plugins": {
    "local": {
      "disabled": true,
      "browsers": [
        "chrome",
        "firefox"
      ]
    },
    "@advanced-rest-client/wct-istanbub": {
      "dir": "./coverage",
      "reporters": [
        "text-summary",
        "lcov"
      ],
      "include": [
        "**/*.js"
      ],
      "exclude": [
        "**/test/**",
        "**/demo/**",
        "**/tasks/**"
      ],
      "thresholds": {
        "global": {
          "statements": 80,
          "branches": 80,
          "functions": 90,
          "lines": 80
        }
      }
    }
  }
}
